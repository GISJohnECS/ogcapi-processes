{
   "id": "EchoProcess",
   "title": "EchoProcess",
   "version": "1.0.0",
   "jobControlOptions": ["async-execute", "sync-execute"],
   "outputTransmission": ["value", "reference"],
   "inputs": [{
      "id": "boundingboxInput",
      "title": "boundingboxInput",
      "schema" : {
         "type" : "array",
         "minItems" : 4, "maxItems" : 6,
         "items" : { "type" : "number", "format" : "double" }
      },
      "supportedCRS": [ {"default": true, "crs": "EPSG:4326"} ],
      "minOccurs": 1,
      "maxOccurs": 1
   },
   {
      "id": "literalInput",
      "title": "literalInput",
      "schemas": {
         "type": "number",
         "format": "double"
      },
      "minOccurs": 1,
      "maxOccurs": 1
   },
   {
      "id": "complexInput",
      "title": "complexInput",
      "schema": {
         "oneOf": [
            { "type": "string", "contentMediaType": "application/xml" },
            { "type": "string", "contentMediaType": "text/xml" }
         ]
      },
      "defaultMediaType": "application/xml",
      "minOccurs": 1,
      "maxOccurs": 1
   }],
   "outputs": [{
      "id": "boundingboxOutput",
      "title": "boundingboxOutput",
      "schema" : {
         "type" : "array",
         "minItems" : 4, "maxItems" : 6,
         "items" : { "type" : "number", "format" : "double" }
      },
      "supportedCRS": [ {"default": true, "crs": "EPSG:4326"} ]
   },
   {
      "id": "literalOutput",
      "title": "literalOutput",
      "schemas": {
         "type": "number",
         "format": "double"
      }
   },
   {
      "id": "complexOutput",
      "title": "complexOutput",
      "schema": {
         "oneOf": [
            { "type": "string", "contentMediaType": "application/xml" },
            { "type": "string", "contentMediaType": "text/xml" }
         ]
      },
      "defaultMediaType": "application/xml"
   }],
   "links": [
      {
        "href": "https://processing.example.org/oapi-p/jobs",
        "rel": "execute",
        "title": "Execute endpoint"
      }
   ]
}
